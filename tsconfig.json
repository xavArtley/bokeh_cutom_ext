{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noEmitOnError": false,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "lib": ["es5", "dom", "es2015.collection"],
    "baseUrl": ".",
    "types": ["node"],
    "paths": {
      "*": ["node_modules/bokehjs/build/js/types/*"]
    },
  }
}